<?xml version='1.0' encoding='UTF-8' ?>
<dialogs>
  <window id='#PB_Any' name='modSettings' text='Mod Settings' minwidth='400' minheight='100' flags='#PB_Window_SystemMenu | #PB_Window_Tool | #PB_Window_WindowCentered'>
    <vbox expand='item:3' spacing='2'>
      <text name='name' text='Settings: Mod Name' flags='#PB_Text_Center' />
      <empty height='3' />
      <frame name='settings' text='Settings'>
        <gridbox id='settings' colexpand='item:3' rowexpand='no' columns='4'>  </gridbox>
      </frame>

      <empty height='8' />

      <hbox expand='equal'>
        <button name='cancel' text='Cancel' />
        <empty />
        <button name='save' text='Save' flags='#PB_Button_Default' />
      </hbox>

    </vbox>
  </window>
</dialogs>
