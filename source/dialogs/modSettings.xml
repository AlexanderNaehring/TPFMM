<?xml version='1.0' encoding='UTF-8' ?>
<dialogs>
  <window id='#PB_Any' name='modSettings' text='Mod Settings' minwidth='400' minheight='400' flags='#PB_Window_SystemMenu | #PB_Window_Tool | #PB_Window_WindowCentered'>
    <vbox expand='item:3' spacing='2'>
      <text name='name' text='Settings: Mod Name' flags='#PB_Text_Center' />
      <empty height='5' />
      <gridbox id='settings' colexpand='item:2' rowexpand='no' columns='3'>
      </gridbox>

      <hbox expand='equal'>
        <button name='save' text='Save' />
        <empty />
        <button name='cancel' text='Cancel' />
      </hbox>

    </vbox>
  </window>
</dialogs>
