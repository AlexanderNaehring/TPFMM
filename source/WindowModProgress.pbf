Global WindowModProgress


Procedure OpenWindowModProgress(x = 0, y = 0, width = 600, height = 400)
  WindowModProgress = OpenWindow(#PB_Any, x, y, width, height, "", #PB_Window_SystemMenu | #PB_Window_Tool | #PB_Window_WindowCentered, WindowID(WindowMain))
  HideWindow(WindowModProgress, 1)
EndProcedure

Procedure WindowModProgress_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

